{% extends 'base.html.twig' %}

{% block body %}
    <h1>Monedatipo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ monedaTipo.id }}</td>
            </tr>
            <tr>
                <th>Moneda</th>
                <td>{{ monedaTipo.moneda }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ monedaTipo.descripcion }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('monedatipo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('monedatipo_edit', { 'id': monedaTipo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
